                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity-alias>
        <activity-alias
            android:name="com.pocket.app.BlackAndWhiteLogoActivity"
            android:enabled="false"
